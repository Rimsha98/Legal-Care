@using (Html.BeginForm("attorney_registration", "accounts", FormMethod.Post, new { encType = "multipart/form-data", @id = "attorney" }))
{
<section class="ftco-section ftco-no-pt ftco-no-pb mt-5 mb-5" id="Register">
    <div class="container p-0" style="width: 80%;">
        <div class="row mb-0 no-gutters w-100 form-group">
            <div class="col-lg-4 float-left float-md-none">
                <button class="form-button p-2" id="Step-1" type="button" onclick="ShowBasic()">Basic Information</button>
            </div>
            <div class="col-lg-4 float-left float-md-none">
                <button class="form-button p-2" id="Step-2" name="next" value="step1next" onclick="Verify_Step1(this)">Professional Information</button>
            </div>
            <div class="col-lg-4 float-left float-md-none">
